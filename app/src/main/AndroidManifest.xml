            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".Activity.Activity_zirenwu" />
        <activity android:name=".Activity.Activity_persion_X" />
        <activity android:name=".Activity.Activity_change_password" />
        <activity android:name=".Activity.Activity_Chat" />
        <activity android:name=".Activity.Activity_zerenren" />
        <activity android:name=".Activity.Activity_add_renwu"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".Activity.Activity_addto_youself" />
        <activity android:name=".Activity.Activity_addRWname" />
        <activity android:name=".Activity.Activity_xiafa"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".Activity.Activity_daishangxiafa" />
        <activity android:name=".Activity.Activity_yiban_deatail" />
        <activity android:name=".Activity.Activity_Xiafa_deatil" />
        <activity android:name=".Activity.Activity_zirenwu_deatil" />
        <activity android:name=".Activity.Activity_zirenwu_deatil2" />
        <activity android:name=".Activity.Activity_zirenwu1" />
        <activity android:name=".Activity.Activity_zirenwu2" />
        <activity android:name=".Activity.Activity_zirenwu3" />
        <activity android:name=".Activity.Activity_zirenwu4" />
        <activity android:name=".Activity.Activity_zirenwu5" />
        <activity android:name=".Activity.Activity_zirenwu6" />
        <activity android:name=".Activity.Activity_zirenwu8" />
        <activity android:name=".Activity.Activity_zirenwu9" />
        <activity android:name=".Activity.Activity_zirenwu10" />
        <activity android:name=".Activity.Activity_zirenwu11" />
        <activity android:name=".Activity.Activity_huibao" />
        <activity android:name=".Activity.Activity_huibao2" />
        <activity android:name=".Activity.Activity_zirenwu7" />
        <activity android:name=".Activity.Activity_Chexiao_deatil" />
        <activity android:name=".Activity.Activity_Zanting_deatil" />
        <activity android:name=".Activity.Activity_tubiao" />
        <activity android:name=".Activity.Activity_yiban_deatail2" />
        <activity android:name=".Activity.Activity_zirenwu23" />
        <activity android:name=".Activity.Activity_zirenwu66"/>
